@inherits Umbraco.Web.Mvc.UmbracoViewPage<OneSchool.Core.UmbracoModels.Course>
@{
    var course = Model as Course;
}

@(Html.RenderFETemplate<vmBlock_CommentList>(new RenderSettings()
    {
        Template = "parForm",
        MapFrom = course.Comments,
        ShowJson = true
    },
    new Dictionary<string, object>() { { "HtmlHelper", Html }, { "Model", Model } }))


